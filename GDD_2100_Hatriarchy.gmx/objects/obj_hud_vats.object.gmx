            last_selected.image_alpha = 1;
            last_selected = s;
        }
        
        s.image_alpha = 0.7;
        
        if (creator.vats_selection_finalized)
        {
            draw_text(50, 120, "loc: " + string(creator.vats_selected_loc));
            var s = creator.vats_selected;
            draw_set_color(c_red);
            switch(creator.vats_selected_loc)
            {
                case LOC_HIGH:
                    var bottom_left_x = s.bbox_left;
                    var bottom_left_y = s.bbox_bottom + (3 * ((s.bbox_top - s.bbox_bottom) / 3));
                    var top_right_x = bottom_left_x + s.bbox_right - s.bbox_left;
                    var top_right_y = bottom_left_y - ((s.bbox_top - s.bbox_bottom) / 3);
                    draw_rectangle(bottom_left_x, bottom_left_y, top_right_x, top_right_y, true);
                    break;
                case LOC_LOW:
                    var bottom_left_x = s.bbox_left;
